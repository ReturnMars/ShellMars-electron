{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "ignores": ["**/node_modules", "**/dist", "**/out"],
  "categories": {
    "correctness": "error",
    "suspicious": "warn",
    "style": "warn"
  },
  "plugins": ["typescript", "vue"],
  "rules": {
    "vue/require-default-prop": "off",
    "vue/multi-word-component-names": "off"
  }
}

